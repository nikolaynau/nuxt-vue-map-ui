{
  "name": "nuxt-vue-map-ui",
  "version": "1.0.7",
  "description": "Vue map ui module for Nuxt",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/nuxt-vue-map-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/nuxt-vue-map-ui.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/nuxt-vue-map-ui/issues"
  },
  "keywords": [
    "vue-map-ui",
    "nuxt"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "scripts": {
    "build": "npm run dev:prepare && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --fix .",
    "lint:test": "eslint .",
    "format": "prettier --write \"**/*.{ts,vue}\"",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "peerDependencies": {
    "leaflet": ">=1.9.0",
    "vue-map-ui": ">=0.0.9",
    "@types/leaflet": ">=1.9.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-angular": "^17.4.4",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.3",
    "@types/leaflet": "^1.9.1",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "leaflet": "^1.9.3",
    "lint-staged": "^13.1.2",
    "nuxt": "^3.2.3",
    "prettier": "^2.8.4",
    "vue-map-ui": "^0.0.10"
  }
}
