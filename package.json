{
  "name": "nuxt-vue-map-ui",
  "version": "1.0.15",
  "description": "Vue map ui module for Nuxt",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/nuxt-vue-map-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/nuxt-vue-map-ui.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/nuxt-vue-map-ui/issues"
  },
  "keywords": [
    "vue-map-ui",
    "nuxt"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "scripts": {
    "build": "npm run dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "lint": "eslint --fix .",
    "lint:test": "eslint .",
    "format": "prettier --write \"**/*.{ts,vue}\"",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "peerDependencies": {
    "@types/leaflet": ">=1.9.0",
    "leaflet": ">=1.9.0",
    "vue-map-ui": ">=0.0.18"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@leaflet-extensions/locatecontrol": "^0.0.4",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@types/leaflet": "^1.9.8",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "leaflet": "^1.9.4",
    "lint-staged": "^15.1.0",
    "nuxt": "^3.8.2",
    "prettier": "^3.1.0",
    "vue-map-ui": "^0.0.18"
  }
}
